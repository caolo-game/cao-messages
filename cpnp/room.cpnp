@0x8fa8617ce0060b4d;

using Point = import "point.cpnp";

struct RoomTerrainMessage {
    tiles @0: List(Tile);
    roomProperties @1: RoomProperties;
}

struct RoomProperties {
    roomRadius @0: UInt32;
    roomId @1: Point.AxialPoint;
}

struct Tile {
    position @0: Point.WorldPosition;
    ty @1: TerrainType;
}

enum TerrainType {
    empty @0;
    wall @1;
    plain @2;
    bridge @3;
}
